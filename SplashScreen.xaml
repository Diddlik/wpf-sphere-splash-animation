    <Grid>
        <!-- Dunkler Hintergrund -->
        <Rectangle Fill="#FF1A1A1A" RadiusX="10" RadiusY="10"/>

        <!-- Canvas für die animierten Kreise -->
        <Canvas x:Name="AnimationCanvas" 
                Width="600" Height="600"
                HorizontalAlignment="Center" 
                VerticalAlignment="Center">
        </Canvas>

        <!-- Vollständiges Logo (wird nach Animation eingeblendet) -->
        <Image x:Name="CompleteLogo"
               Source="{StaticResource dgo_new_logo}"
               Width="300" Height="300"
               HorizontalAlignment="Center"
               VerticalAlignment="Center"
               Opacity="0"
               RenderOptions.BitmapScalingMode="HighQuality"/>

        <!-- Loading Text -->
        <TextBlock x:Name="LoadingText"
                   Text="Laden..."
                   FontSize="20"
                   Foreground="#FF9E9E9E"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Bottom"
                   Margin="0,0,0,50"
                   Opacity="0">
            <TextBlock.RenderTransform>
                <TranslateTransform Y="20"/>
            </TextBlock.RenderTransform>
        </TextBlock>
    </Grid>
</Window>